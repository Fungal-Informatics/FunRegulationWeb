type BoardType enum { task notes bookmarks }

type SimpleUser {
    id: string
    name: string
}

type Board {
    id: uuid
    ownedByMe: bool
    name: string
    type: BoardType?
    sharedWith: SimpleUser[]?
    ...Metadata
}

fn myBoards(): Board[]
fn getBoard(boardId: string): Board
fn createBoard(name: string, type: BoardType): void
fn updateBoard(boardId: string, name: string): void
fn archiveBoard(boardId: string): void

fn shareBoard(boardId: string, email: string): void
