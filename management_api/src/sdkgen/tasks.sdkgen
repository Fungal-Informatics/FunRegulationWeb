
type NewTask {
    title: string
    done: bool
}

type Task {
    id: uuid
    ...NewTask
}

type UpdateTask {
    title: string
    done: bool
    boardId: uuid
}

fn getTasks(boardId: string): Task[]
fn createTask(boardId: string, task: NewTask): Task
fn updateTask(taskId: string, task: UpdateTask): void
fn archiveTask(taskId: string): void
